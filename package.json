{
  "name": "modular-bot",
  "version": "0.0.1-a",
  "description": "Modular Bot",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "type-check": "tsc --noEmit",
    "test": "NODE_ENV=test nyc --reporter=html mocha --recursive --require test/fixtures/babel.js --require chai/register-expect --require sinon-register --file test/fixtures test/unit test/integration",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint-fix": "npm run lint -- --fix",
    "build": "npm run build:js && npm run build:types && npm run build:merge-types",
    "build:merge-types": "cp -r ./dist/src/* ./dist && rm -rf ./dist/{dist,src}",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src -d dist --extensions \".ts,.js\"  --source-maps inline",
    "prepare": "npm run build",
    "dev": "babel-node --extensions \".ts,.js\" src/bin/discord-bot.ts"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@types/chai": "^4.2.14",
    "@types/sinon": "^9.0.10",
    "@types/triple-beam": "^1.3.2",
    "axios": "^0.21.1",
    "babel-plugin-istanbul": "^6.0.0",
    "body-parser": "^1.19.0",
    "boolean": "^3.0.2",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "logform": "^2.2.0",
    "sinon-register": "^1.0.1",
    "triple-beam": "^1.3.0",
    "typescript": "^4.1.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/eslint-plugin": "^7.12.13",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@types/mocha": "^8.2.0",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "chai": "^4.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "totally-di": "^0.2.2"
  },
  "nyc": {
    "check-coverage": false,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 50,
    "cache": true,
    "all": true,
    "exclude": [
      "coverage",
      "test"
    ]
  }
}
