{
  "name": "core-service",
  "version": "0.0.1-a",
  "description": "Filing Findings core service",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "nyc --reporter=html mocha --require @babel/register --file test/fixtures test/unit/* test/integration/*",
    "lint": "eslint \"src/**/*.js\"",
    "lint-fix": "npm run lint -- --fix",
    "build": "babel src -d dist --copy-files",
    "prepare": "npm run build",
    "dev": "babel-node src/bin/discord-bot.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "di-namic": "^0.4.0",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "logform": "^2.2.0",
    "mongoose": "^5.11.9",
    "totally-di": "^0.1.0",
    "triple-beam": "^1.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "chai": "^4.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0"
  },
  "nyc": {
    "check-coverage": false,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 50,
    "cache": true,
    "all": true,
    "exclude": [
      "coverage",
      "test"
    ]
  }
}
